<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet id="12" author="pratheepg">
        <preConditions onFail="MARK_RAN">
            <sqlCheck expectedResult="0">
                SELECT COUNT(*) FROM identity_provider_auth_type WHERE id = 1
            </sqlCheck>
        </preConditions>

        <insert tableName="identity_provider_auth_type">
            <column name="name" value="User-Name Password Login"/>
            <column name="is_active" valueBoolean="true"/>
            <column name="first_factor_id" value="OOB_STATIC_PWD"/>
            <column name="identity_provider_id" value="SC_IDP"/>
        </insert>
    </changeSet>
</databaseChangeLog>