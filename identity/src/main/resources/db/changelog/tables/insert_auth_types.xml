    <changeSet id="5" author="pratheepg">
        <preConditions onFail="MARK_RAN">
            <sqlCheck expectedResult="0">
                SELECT COUNT(*) FROM auth_type WHERE id = 'OOB_STATIC_PWD'
            </sqlCheck>
        </preConditions>
        <insert tableName="auth_type">
            <column name="id" value="OOB_STATIC_PWD"/>
            <column name="name" value="OOB_STATIC_PWD"/>
            <column name="description" value="Authentication using Username and Password"/>
            <column name="is_active" valueBoolean="true"/>
            <column name="auth_adapter" value="STATIC_PWD"/>
            <column name="config" value='{"maxLoginAttempts":3,"captchaNeededOnAttempts":2,"isCaptchaEnabled":true,"isAutoUnlockEnabled":true,"autoUnLockAfterInMinutes":30,"passwordExpiryInMinutes":7200}' />
        </insert>
    </changeSet>

